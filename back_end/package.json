{
  "name": "will-power-back_end",
  "description": "",
  "version": "0.0.0",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "JohnnRen",
    "email": "john.ren.contact@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && NODE_ENV= npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "clean-data": "rm -rf ./data && mkdir ./data",
    "init": "npm run clean-data && cd ./init && node initEquipmentDb.js && node initMonsterDb.js",
    "start": "cross-env NODE_ENV=production node src/",
    "dev": "cross-env NODE_ENV=development nodemon src/",
    "prod": "pm2 restart ecosystem.config.js --env production",
    "prod-test": "pm2 restart ecosystem.config.js --env development",
    "prod-stop": "pm2 stop ecosystem.config.js",
    "prod-delete": "pm2 delete ecosystem.config.js"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.16",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^1.2.9",
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "^3.3.6",
    "@feathersjs/express": "^1.3.1",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio": "^3.2.9",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "express-request-logger": "0.0.3",
    "feathers-nedb": "^4.0.1",
    "fs-extra": "^8.0.1",
    "helmet": "^3.18.0",
    "moment": "^2.24.0",
    "ncp": "^2.0.0",
    "nedb": "^1.8.0",
    "node-schedule": "^1.3.2",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.0",
    "fs-extra-cli": "0.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
