{
  "name": "will-power-front_end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy_clean": "fsys --task=emptyDir --dir=../back_end/public",
    "deploy_copy_dist": "fsys --task=copy --filesFolders=dist/ --destination=../back_end/ --clobber='true' --preserveTimestamps='false'",
    "deploy": "npm run build && npm run deploy_clean && npm run deploy_copy_dist"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^1.0.11",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio-client": "^1.2.1",
    "@tweenjs/tween.js": "^17.3.0",
    "core-js": "^2.6.5",
    "echarts": "^4.2.1",
    "element-ui": "^2.8.2",
    "feathers-vuex": "^1.7.0",
    "moment": "^2.24.0",
    "pixi-viewport": "^3.21.0",
    "pixi.js": "^4.8.7",
    "prettier-eslint": "^8.8.2",
    "socket.io-client": "^2.2.0",
    "v-charts": "^1.19.0",
    "vue": "^2.6.10",
    "vue-easeljs": "^0.1.1",
    "vue-functional-calendar": "^2.3.4",
    "vue-router": "^3.0.3",
    "vue-typed-js": "^0.1.2",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "fs-extra-cli": "0.0.4",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
